@FunctionalInterface
interface Add

{
	void add();
}
@FunctionalInterface
interface Sub
{
	int sub(int num1);
}
public class TestLambda {

	public static void main(String[] args) {
		Add a=()->{
			int num1=10;
			int num2=30;
			int result=num1+num2;
			System.out.println(result);
		};
		
		a.add();
		
		Sub s = (int num1)->{
			int res=num1-5;
			return res;
		};
		System.out.println(s.sub(500));

	}

}
