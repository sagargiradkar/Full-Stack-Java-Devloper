import java.util.*;
import java.util.Map.Entry;
class Student 
{
	private String name;
	private int marks;
	private int age;
	private String city;
	public Student(String name, int marks, int age, String city) {
		super();
		this.name = name;
		this.marks = marks;
		this.age = age;
		this.city = city;
	}
	public String getName() {
		return name;
	}
	public int getMarks() {
		return marks;
	}
	public int getAge() {
		return age;
	}
	public String getCity() {
		return city;
	}
	@Override
	public String toString()
	{
		return name + " "+ marks+" "+age+" "+city;
	}
}
public class LaunchmapComplex {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student s1 = new Student("Sagar",95,22,"Nagur");
		Student s2 = new Student("Rahul",100,22,"Kolhapur");
		Student s3 = new Student("Ram",25,21,"Pune");
		
		HashMap hm = new HashMap();
		
		hm.put(1, s1);
		hm.put(2, s2);
		hm.put(3, s3);
		
		 Collection col= hm.values();
		 Iterator itr1 = col.iterator();
		 while(itr1.hasNext())
		 {
			 System.out.println("Values of students "+itr1.next());
		 }
		System.out.println("*********************************");
		Set s12 = hm.keySet();
		Iterator itr2 = s12.iterator();
		while(itr2.hasNext())
		{
			System.out.println("Keys of students :"+itr2.next());
		}
		Set s = hm.entrySet();
		Iterator itr = s.iterator();
		while(itr.hasNext())
		{
			Map.Entry data =  (Entry) itr.next();
			System.out.println(data.getValue());
		}
		//System.out.println(hm);
	}

}
