interface Interf
{
	public void m1(int i);
}

public class InterFace {
	public void logicCoded(int i)
	{
		System.out.println("From method reference");
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Interf i = x -> System.out.println("Lambada Expression");
		i.m1(100);
		
		System.out.println();
		
		Interf i1 = new InterFace() :: logicCoded;
		i1.m1(10);
	}

}
