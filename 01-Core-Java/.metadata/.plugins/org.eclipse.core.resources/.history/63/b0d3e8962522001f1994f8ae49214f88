		 * Iterator itr = al.iterator();
		 * 
		 * while(itr.hasNext()) { System.out.print(itr.next()+" "); al.add(123); }
		 */
		//Failssafe
		
		CopyOnWriteArrayList cal = new CopyOnWriteArrayList();
		
		cal.add(5000);
		cal.add(15000);
		cal.add(25000);
		cal.add(35000);
		cal.add(45000);
		cal.add(55000);
		cal.add(65000);
		cal.add(75000);
		
		Iterator itr1 = cal.iterator();
		
		while(itr1.hasNext())
		{
			System.out.println(itr1.next());
			cal.add(1225);
		}
	}

}
