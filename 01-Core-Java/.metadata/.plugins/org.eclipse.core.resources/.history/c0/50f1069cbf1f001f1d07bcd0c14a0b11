/*
Custome exception



*/

import java.util.Scanner;

class Atm
{
	int userId=1212;
	int password = 1111;
	int pw;
	int uid;
	
	public void input()
	{
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Kindly entre use id");
		uid = sc.nextInt();
		
		System.out.println("Kindly Eneter your password :: ");
		pw=sc.nextInt();
		
	}
	public void verify()
	{
		if((userId==uid) && (password==pw)) {
			System.out.println("Take your cash");
		}
		else
		{
			System.out.println("Are you the owner of card or not");
		}
	}
	
}

class Bank
{
	public void initiate()
	{
		Atm a = new Atm();
		a.input();
		a.verify();
	}
}

public class CustomExceptions {
	
	public static void main(String[] args) {
		System.out.println("Custom Exceptions");
		Bank b = new Bank();
		b.initiate();
	}
}
