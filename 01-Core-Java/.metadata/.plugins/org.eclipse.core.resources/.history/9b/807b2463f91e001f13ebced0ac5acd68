public class AutoBoxing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Wrapper class
//		Number => Byte ,Short ,Integer,Long,Float(3) ,Double
//		Character(1),Boolean(2)
//						|=>"TRUE"
//		
//		equals()=> cpmpaire the content 
//		toString()=>To print
		
//		a.valueOf()=>String/primitive=>Wrapper Typw
//		b.xxxValue()=>Wrapper===========>primitive type
//		c.parseXXX()=> String ==========> primitive type
//		d.toString()=> Primitive/Wrapper =====>String
		
//		AutoBoxuig => valueOf()=>Internally use buffer object to improve performance
//					createing constructor
//		AutoUnBoxing==>xxxvValue()
		
		Demo d = new Demo();
		d.add(25);
		
	}

}
