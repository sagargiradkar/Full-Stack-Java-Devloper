class ThreadB extends Thread
{
	int total = 0;
	@Override
	public void run()
	
	{
		synchronized(this) 
		{
			System.out.println("Child Thread Started Calculation .....");
		for(int i=0 ; i<=100 ;i++)
		{
			total+=i;
		}
		System.out.println("Child thread give notificayopn call");
		this.notify();
		}
	}
}
public class Test5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ThreadB b = new ThreadB();
		b.start();
	}

}
