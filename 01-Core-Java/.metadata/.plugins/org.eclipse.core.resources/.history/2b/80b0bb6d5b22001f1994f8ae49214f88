import java.util.*;

class Employee
{
	private int empid;
	private String name;
	private String empAddr;
	@Override
	public String toString()
	{
		return "sagar";
	}
	
	@Override
	public void finalize()
	{
		System.out.println("Garbage collector collected the object properly");
	}
	
}
public class LaunchGC {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Employee e = new Employee();
		
		
		e=null;//elligible for garbege collector
		System.gc();
	}

}
