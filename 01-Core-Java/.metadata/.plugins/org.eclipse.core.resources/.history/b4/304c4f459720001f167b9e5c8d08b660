class myThread extends Thread
{
	@Override
	public void run()
	{
		//Job of a thread 
		for(int i=0;i<=10;i++)
		{
			System.out.println("Chlid Thread"+i);
		}
	}
}
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		myThread t = new myThread();
		t.run();
	}

}
