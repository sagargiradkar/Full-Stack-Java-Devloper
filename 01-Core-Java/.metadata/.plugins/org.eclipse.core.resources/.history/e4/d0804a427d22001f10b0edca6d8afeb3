enum Course
{
	JAVA,JEE,SPRINGBOOT;
	int courseId;
	Course()
	{
		
	}
	private Course(int courseId) {
		this.courseId = courseId;
	}
	void setCourseId(int courseId)
	{
		this.courseId=courseId;
	}
	int getCourseId()
	{
		return courseId;
	}
}
public class Enum3 {

	public static void main(String[] args) {
		
		Course.JAVA.setCourseId(1000);
		
		int cid = Course.JAVA.getCourseId();
		System.out.println("Course Id :: "+cid);

	}

}
