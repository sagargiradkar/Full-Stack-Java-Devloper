class Demo1 
{
	void disp()
	{
		try {
		System.out.println("Method Started");
		System.exit(0);
		}
		
		finally {
			System.out.println("Method Endided");
		}
		
	}
}
public class LaunchFinallyExits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Demo1 d = new Demo1();
		d.disp();
	}

}
