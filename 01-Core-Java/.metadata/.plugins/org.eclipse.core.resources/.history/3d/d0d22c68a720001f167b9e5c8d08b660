class MyThread extends Thread
{
	@Override
	public void run()
	{
		//Job of a thread 
		for(int i=1;i<=10;i++)
		{
			System.out.println("Chlid Thread :: "+i);
		}
	}
}
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MyThread t = new MyThread();
		t.start();
		
		for(int i=1;i<=10;i++)
		{
			System.out.println("In Main thread");
		}
	}

}
