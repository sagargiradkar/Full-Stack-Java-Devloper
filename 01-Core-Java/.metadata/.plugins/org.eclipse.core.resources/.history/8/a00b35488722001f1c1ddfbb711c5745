/*
 * 
 * Normal class :: A class without any TypeParameter 
 * 
 * Generic Class :: A class with TypeParameter
class ArrayList
{
	add(Objevt o);
	Object get(int index);
	
}
ArrayList al = new ArrayList();
al.add("Sagar");
al.add(new Integer(10));
Integer i = (Integer)al.get(1);

String data = (String) al.get(0);
Generic  JDK 1.5 version 
====================================
	|=> TypeParameter 
class ArrayList<T>
{
	add(T t);        ============> add(String t)
	
	T get(int index);============> String get(int index)
}

ArrayList<T> al = new ArrayList<T>();

ArrayList<String> al = new ArrayList<String>();
	al.add("Sachin");
	String data = al.get(0);
	
*/
import java.util.*;

/*

Predefined - api 

ArrayList<T>
LinkedList<T>
HashSet<T>
LinkedHashSet<T>

*/
/*class Account<T>
{
	
}
new Account<Gold> acc1 = new Account <Gold>();
new Account<Platinum> acc1 = new Account <Platinum>();
*/

class Gen <T>
{
	T obj; //Integer obj
	Gen(T obj)
	{
		this.obj=obj;
	}
	public void show()
	{
		System.out.println("The type of objevt is :: "+obj.getClass().getName());
	}
	public T getObj()
	{
		return obj;
	}
}
public class Generic1 {

	public static <T> void main(String[] args) {
		// TODO Auto-generated method stub
		//ArrayList<T> al = new ArrayList<T>();
		Gen<Integer> g1 = new Gen<Integer>(10);
		g1.show();
		System.out.println(g1.getObj());
		System.out.println();
		
		Gen<String> g2 = new Gen<String>("Sagar");
		g2.show();
		System.out.println(g2.getObj());
		System.out.println();
		Gen<Integer> g3 = new Gen<Integer>(10);
		g3.show();
		System.out.println(g3.getObj());
		System.out.println();
	}

}
