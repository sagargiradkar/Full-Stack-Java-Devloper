import java.util.*;import java.util.stream.Collectors;
public class StreamAPI2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList<Integer> al = new ArrayList<Integer>();
		al.add(0);
		al.add(5);
		al.add(10);
		al.add(15);
		al.add(20);
		al.add(25);
		al.add(30);
		al.add(35);
		System.out.println(al);
		
		ArrayList<Integer> doubleList = new ArrayList<Integer>();
		for(Integer l1 :al)
			doubleList.add(l1*2);
		System.out.println(doubleList);
		
		List<Integer> stramList = al.stream().map(obj->obj*2).collect(Collectors.toList());
		
		System.out.println(stramList);
		System.out.println();
		stramList.forEach(i-> System.out.println(i));
		stramList.forEach(System.out::println);
	}

}
