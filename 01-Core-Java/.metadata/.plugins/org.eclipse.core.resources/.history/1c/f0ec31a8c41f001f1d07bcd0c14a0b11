import java.util.Scanner;
class UnderAgeException extends Exception
{
	public UnderAgeException(String msg)
	{
		super(msg);
	}
}
class OverAgeException extends Exception
{
	public OverAgeException(String msg)
	{
		super(msg);
	}
}
class Applicant 
{
	int age;
	public void input()
	{
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter Age :: ");
		age=sc.nextInt();
	}
	void verify() throws UnderAgeException, OverAgeException 
	{
		if(age<18)
		{
			UnderAgeException uae = new UnderAgeException("Ohh dude your age not fgreter that 18");
			System.out.println(uae);
			throw uae;
		}
		else if(age>60)
		{
			OverAgeException oae = new OverAgeException("You are greter than 60");
			System.out.println(oae);
			throw oae;
		}
		else
		{
			System.out.println("You are elligibel");
		}
	}
}
class Rto
{
	public void initiate()
	{
		Applicant a = new Applicant();
		
		a.input();
		try {
			a.verify();
		} catch (UnderAgeException | OverAgeException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			a.input();
			try {
				a.verify();
			} catch (UnderAgeException | OverAgeException e1) {
				// TODO Auto-generated catch block
				e1.printStackTrace();
				System.out.println("Dont again try again read rules");
				System.exit(0);
			}
		}
	}
}
public class LaunchRTO {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
