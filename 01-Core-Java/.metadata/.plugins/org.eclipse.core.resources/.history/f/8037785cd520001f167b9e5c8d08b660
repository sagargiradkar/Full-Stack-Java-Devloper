
class MyThread1 extends Thread
{
	public void run()
	{
		System.out.println("Run method called ......");
	}
}
public class Test1 {
	
	public static void main(String[] args) {
		MyThread1 t = new MyThread1();
		
		Thread t1 = new Thread();
		
		t1.start();
		
		System.out.println("Main Thread .....");
	}
}
